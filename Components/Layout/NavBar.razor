<MudAppBar Color="Color.Warning">
	<MudGrid Justify="Justify.SpaceBetween">
		<MudItem>
			<MudIconButton OnClick="@(() => OnMenuToggle.InvokeAsync())" Icon="@Icons.Material.Filled.Menu" Color="Color.Inherit" />
		</MudItem>
		<MudItem Class="d-flex">
			<LanguageSelector />
		</MudItem>
	</MudGrid>
</MudAppBar>

@code {
	[Parameter]
	public EventCallback OnMenuToggle { get; set; }
}